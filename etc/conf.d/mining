MINING_PASS="password"
MINING_URL="http://mining.bitcoin.cz"
PROG="bfgminer"

OPTS="-dmS ${PROG} ${PROG} ${OPTS} -o ${MINING_URL} -u ${MINING_USER} -p ${MINING_PASS} --api-listen --api-network --api-allow W:192.168.1/24"

# nice level
NICELEVEL="19"

